mDJ"},"outputs":[],"source":["train_df_52 = train_df_52.drop(columns=['day_of_year', 'time'])\n","test_df_52 = test_df_52.drop(columns=['day_of_year', 'time'])"]},{"cell_type":"markdown","metadata":{"id":"I1jnGT_0Wpt6"},"source":["\n","\n","---\n","### 53번 건물\n"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"background_save":true},"id":"XSKRW3jCWpt7"},"outputs":[{"data":{"text/html":["\n","  \u003cdiv id=\"df-c97e41c2-5df8-4914-bcb9-a9272407b80b\" class=\"colab-df-container\"\u003e\n","    \u003cdiv\u003e\n","\u003cstyle scoped\u003e\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","\u003c/style\u003e\n","\u003ctable border=\"1\" class=\"dataframe\"\u003e\n","  \u003cthead\u003e\n","    \u003ctr style=\"text-align: right;\"\u003e\n","      \u003cth\u003e\u003c/th\u003e\n","      \u003cth\u003epower\u003c/th\u003e\n","      \u003cth\u003ebuilding_num\u003c/th\u003e\n","      \u003cth\u003etime\u003c/th\u003e\n","      \u003cth\u003eday\u003c/th\u003e\n","      \u003cth\u003emonth\u003c/th\u003e\n","      \u003cth\u003eweek\u003c/th\u003e\n","      \u003cth\u003eday_of_year\u003c/th\u003e\n","      \u003cth\u003esin_time\u003c/th\u003e\n","      \u003cth\u003ecos_time\u003c/th\u003e\n","      \u003cth\u003etemp_RM\u003c/th\u003e\n","      \u003cth\u003e...\u003c/th\u003e\n","      \u003cth\u003etemp_EMA\u003c/th\u003e\n","      \u003cth\u003ewind_EMA\u003c/th\u003e\n","      \u003cth\u003ehum_EMA\u003c/th\u003e\n","      \u003cth\u003epower_change_rate\u003c/th\u003e\n","      \u003cth\u003eTHI\u003c/th\u003e\n","      \u003cth\u003efeels_temp\u003c/th\u003e\n","      \u003cth\u003eCDH\u003c/th\u003e\n","      \u003cth\u003eday_hour_mean\u003c/th\u003e\n","      \u003cth\u003eheat_index\u003c/th\u003e\n","      \u003cth\u003eheat_wave\u003c/th\u003e